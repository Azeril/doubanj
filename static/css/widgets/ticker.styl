
.ticker
  height: 20px;

  h4
    text-align: right;
    margin: 0;
    font-size: 14px;

.ticker-content
  line-height: 1em;
  height: 20px;
  float: left;

  ul, p
    padding: 0;
    margin: 0;

  li
    margin: 0;
    display: inline;
    list-style: none;

  a
    color: #999;
    display: inline-block;
    margin-right: 0.7em;
    &:hover
      color: #333;
      text-decoration: none;
      border-bottom: 1px dotted #ccc;
