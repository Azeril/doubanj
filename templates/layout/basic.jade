!!!
html
  head
    meta(charset='utf-8')
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    block title
      title #{typeof title === 'undefined' ? conf.site_name : title}
    link(rel='stylesheet', href=static('/css/bootstrap.css'))
    link(rel='stylesheet', href=static('/css/base.css'))
    script(src=static('/js/do.js'), data-cfg-corelib='main', data-cfg-root=static('/'))
    script
      Do.urls(!{urlmap('main')});
  body
    block header
      block navbar
        include ../common/navbar
    .container
      block main
    block footer
    block ending

